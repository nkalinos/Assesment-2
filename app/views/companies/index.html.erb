<%= link_to "Create New Company", new_company_path %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Created At</th>
      <th>Updated At</th>

    </tr>
  </thead>
  <tbody>
    <% @companies.each do |company| %>
      <tr>
        <td> <%= link_to company.company_name, company_path(company) %> </td>
        <td> <%= company.created_at %> </td>
        <td> <%= company.updated_at %> </td>
      </tr>
      <% end %>
  </tbody>
</table>
