<h1> <%= @industry.name  %> </h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Created At</th>
      <th>Updated At</th>

    </tr>
  </thead>
  <tbody>
    <% @industries.each do |industry| %>
      <tr>
        <td> <%= industry.name %> </td>
        <td> <%= industry.description %>
        <td> <%= industry.created_at %> </td>
        <td> <%= industry.updated_at %> </td>
      </tr>
      <% end %>
  </tbody>
</table>
